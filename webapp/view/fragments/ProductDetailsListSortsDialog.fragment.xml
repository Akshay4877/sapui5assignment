<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleProductDetailsSortDialogConfirm">
		<sortItems>
			<ViewSettingsItem text="{i18n>productID}" key="ProductID" selected="true" />
			<ViewSettingsItem text="{i18n>quantity}" key="Quantity" />
			<ViewSettingsItem text="{i18n>unitPrice}" key="UnitPrice" />
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>